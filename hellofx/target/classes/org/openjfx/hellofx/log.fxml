<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.hellofx.LogController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="309.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
      <children>
            <Label layoutX="57.0" layoutY="51.0" prefHeight="26.0" prefWidth="99.0" text="User Action Log:" />
            <Hyperlink layoutX="496.0" layoutY="14.0" onAction="#switchToPostLogin" text="Back" />
            <Button fx:id="showlog" contentDisplay="CENTER" layoutX="183.0" layoutY="51.0" mnemonicParsing="false" onAction="#printlog" text="Show" textFill="WHITE" />
            <TextArea fx:id="log" layoutX="53.0" layoutY="94.0" prefHeight="216.0" prefWidth="568.0" text="Log: " />
      </children>
    </AnchorPane>
  </children>
</VBox>
